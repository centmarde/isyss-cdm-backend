{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/isyss/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/isyss/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/isyss",
  "projects": {
    "isyss": {
      "type": "application",
      "root": "apps/isyss",
      "entryFile": "main",
      "sourceRoot": "apps/isyss/src",
      "compilerOptions": {
        "tsConfigPath": "apps/isyss/tsconfig.app.json"
      }
    },
    "other": {
      "type": "application",
      "root": "apps/other",
      "entryFile": "main",
      "sourceRoot": "apps/other/src",
      "compilerOptions": {
        "tsConfigPath": "apps/other/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    }
  }
}
